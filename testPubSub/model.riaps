// RIAPS Test
// Two actors, A and B, subscribe to each other

app testPubSub {
    message topicA;
    message topicB;

    component A {
      pub pubPort : topicA;
      sub subPort : topicB;
      timer clock 10000;
    }

    component B {
      sub subPort : topicA;
      pub pubPort : topicB;
    }

    actor actorA {
       {
          compA : A;
       }
    }

    actor actorB {
       {
          compB : B;
       }
    }
}
